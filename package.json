{
  "name": "cip-plus",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "clean:node_modules": "find . -name \"node_modules\" -type d -prune -exec rm -r '{}' + ",
    "clean:locks": "find . -name \"pnpm-lock.yaml\" -prune -exec rm -r '{}' + ",
    "preinstall": "node ./scripts/preinstall.js",
    "dev-js": "rollup -c ./build/tasks/js.ts  -w --configPlugin typescript",
    "build-js": "rollup -c ./build/tasks/js.ts --configPlugin typescript",
    "dev-styles": "gulp --require @esbuild-kit/cjs-loader -f ./build/tasks/styles/gulpfile.dev.ts",
    "build-styles": "gulp --require @esbuild-kit/cjs-loader -f ./build/tasks/styles/gulpfile.ts",
    "build-types": "node ./build/tasks/types.js",
    "dev:code-mirror": "cross-env ENTRY_MODULE=cip-code-mirror npm run dev-js",
    "build:code-mirror": "cross-env ENTRY_MODULE=cip-code-mirror pnpm run '/^(build-js|build-types)$/'",
    "dev:micro-app": "cross-env ENTRY_MODULE=cip-micro-app npm run dev-js",
    "build:micro-app": "cross-env ENTRY_MODULE=cip-micro-app npm run build-js",
    "build:charts": "cross-env ENTRY_MODULE=cip-charts pnpm run '/^(build-js|build-types)$/'",
    "build:xdp:button": "cross-env ENTRY_MODULE=xdp/button pnpm run '/^(build-js|build-styles)$/'",
    "build:xdp:config": "cross-env ENTRY_MODULE=xdp/config pnpm run build-js",
    "build:xdp:store": "cross-env ENTRY_MODULE=xdp/store pnpm run build-js",
    "build:xdp:utils": "cross-env ENTRY_MODULE=xdp/utils pnpm run build-js",
    "build:shared": "cross-env ENTRY_MODULE=cip-shared pnpm run build-js",
    "dev:components": "cross-env ENTRY_MODULE=cip-components npm run dev-js",
    "build:components": "cross-env ENTRY_MODULE=cip-components pnpm run '/^(build-js|build-types)$/' && pnpm --filter @xigefish/components gen",
    "build:store": "cross-env ENTRY_MODULE=cip-store pnpm run build-js",
    "dev:styles": "cross-env ENTRY_MODULE=cip-styles pnpm run dev-styles",
    "build:styles": "cross-env ENTRY_MODULE=cip-styles pnpm run build-styles",
    "dev:hooks": "cross-env ENTRY_MODULE=cip-hooks npm run dev-js",
    "build:hooks": "cross-env ENTRY_MODULE=cip-hooks pnpm run build-js",
    "build:request": "cross-env ENTRY_MODULE=cip-request pnpm run build-js",
    "build:utils": "cross-env ENTRY_MODULE=cip-utils pnpm run build-js",
    "dev:@xigefish/d-render-plugin-cci:core": "cross-env ENTRY_MODULE=d-render-plugin-cci/core pnpm run '/^(dev-js|dev-styles)$/'",
    "build:@xigefish/d-render-plugin-cci:core": "cross-env ENTRY_MODULE=d-render-plugin-cci/core pnpm run '/^(build-js|build-styles)$/'",
    "dev:@xigefish/d-render-plugin-cci:configure": "cross-env ENTRY_MODULE=d-render-plugin-cci/configure pnpm run dev-js",
    "build:@xigefish/d-render-plugin-cci:configure": "cross-env ENTRY_MODULE=d-render-plugin-cci/configure pnpm run build-js",
    "build:@xigefish/d-render-plugin-cci:mobile": "cross-env ENTRY_MODULE=d-render-plugin-cci/mobile pnpm run '/^(build-js|build-styles)$/'",
    "build:page-layout:core": "cross-env ENTRY_MODULE=page-layout/core pnpm run build-js",
    "build:page-layout:shared": "cross-env ENTRY_MODULE=page-layout/shared pnpm run build-js",
    "build:page-layout:theme-standard": "cross-env ENTRY_MODULE=page-layout/theme-standard pnpm run '/^(build-js|build-styles)$/'",
    "dev:page-layout:theme-dg": "cross-env ENTRY_MODULE=page-layout/theme-dg pnpm run '/^(dev-js|dev-styles)$/'",
    "build:page-layout:theme-dg": "cross-env ENTRY_MODULE=page-layout/theme-dg pnpm run '/^(build-js|build-styles)$/'",
    "dev:page-layout:theme-supergravity": "cross-env ENTRY_MODULE=page-layout/theme-supergravity pnpm run '/^(dev-js|dev-styles)$/'",
    "build:page-layout:theme-supergravity": "cross-env ENTRY_MODULE=page-layout/theme-supergravity pnpm run '/^(build-js|build-styles)$/'",
    "dev:d-render": "cross-env ENTRY_MODULE=d-render pnpm run '/^(dev-js|dev-styles)$/'",
    "build:d-render": "cross-env ENTRY_MODULE=d-render pnpm run '/^(build-js|build-styles)$/'",
    "dev:d-render-design": "cross-env ENTRY_MODULE=design pnpm run '/^(dev-js|dev-styles)$/'",
    "build:d-render-design": "cross-env ENTRY_MODULE=design pnpm run '/^(build-js|build-styles)$/'",
    "dev:d-render-shared": "cross-env ENTRY_MODULE=shared pnpm run '/^(dev-js)$/'",
    "build:d-render-shared": "cross-env ENTRY_MODULE=shared pnpm run '/^(build-js)$/'",
    "demo": "pnpm -C play serve",
    "demo:base": "pnpm -C micro-app/base serve",
    "demo:sub2": "pnpm -C micro-app/sub-vue2 serve",
    "demo:sub3": "pnpm -C micro-app/sub-vue3 serve",
    "demo-sub": "run-p demo:*",
    "doc": "pnpm -C play build && scp -r play/dist/* 10.162.16.21:~/web_html/cip-example/",
    "build-all": "run-s build:**",
    "dev-all": "run-p dev:*",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@esbuild-kit/cjs-loader": "^2.3.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/pluginutils": "^4.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-clean-css": "^4.3.0",
    "@types/gulp-less": "^0.0.32",
    "@types/gulp-rename": "^2.0.1",
    "@types/node": "^18.7.3",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vitest/coverage-v8": "^0.34.6",
    "chalk": "^4.1.2",
    "change-case": "^4.1.2",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.15.7",
    "eslint": "^7.32.0",
    "estree-walker": "2.0.2",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-less": "^5.0.0",
    "happy-dom": "^12.9.1",
    "jsdom": "^22.1.0",
    "less": "^4.1.3",
    "magic-string": "^0.26.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.78.0",
    "rollup-plugin-deps-external": "^0.2.1",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.4.0",
    "typescript": "^5.2.2",
    "vite": "^3.0.4",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.6",
    "vue": "3.x",
    "vue-tsc": "^1.8.18"
  },
  "dependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "glob": "8.x",
    "gulp-rename": "^2.0.0",
    "lodash-es": "^4.17.21",
    "rimraf": "^5.0.0"
  },
  "publishConfig": {
    "directory": "./packages"
  }
}
